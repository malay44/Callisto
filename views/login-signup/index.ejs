<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>CALLISTO</title>
  <link rel="stylesheet" href="/static/login-signup/style.css">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0">
  <meta charset="utf-8">
  <link rel="stylesheet" type="text/css" href="main.css">
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700;800&display=swap" rel="stylesheet">
</head>

<body>
  <!-- partial:index.partial.html -->

  <div class="main">
    <div class="container a-container" id="a-container">

      <form class="form" id="a-form" method="post" action="register">
        <h2 class="form_title title">Create Account</h2>
        <div class="form__icons">
          <a href="https://callistox.herokuapp.com/auth/google">
            <img class="form__icon"
              src="data:image/svg+xml;base64,"
              alt="google icon">
          </a>
        </div>
        <span class="form__span">or use email for registration</span>
        <input class="form__input" type="text" name="uname" placeholder="Username" required>
        <input class="form__input" type="text" name="name" placeholder="Name" required>
        <input class="form__input" type="text" name="Email" placeholder="Email" required>
        <input class="form__input" type="password" name="pw" placeholder="Password" required>
        <button type="submit" class="form__button button submit">SIGN UP</button>
      </form>
    </div>
    <div class="container b-container" id="b-container">
      <form class="form" id="b-form" method="post" action="/login">
        <h2 class="form_title title">Sign in to Website</h2>
        <div class="form__icons">
          <a href="https://callistox.herokuapp.com/auth/google">
            <img class="form__icon"
              src="data:image/svg+xml;base64,"
              alt="google icon">
          </a>
        </div>
        <span class="form__span">or use your email account</span>
        <input class="form__input" type="text" name="uname" placeholder="Email">
        <input class="form__input" type="password" name="pw" placeholder="Password"><a class="form__link">Forgot your
          password?</a>
        <button type="submit" class="form__button button submit">SIGN IN</button>
      </form>
    </div>
    <div class="switch" id="switch-cnt">
      <div class="switch__circle"></div>
      <div class="switch__circle switch__circle--t"></div>
      <div class="switch__container" id="switch-c1">
        <h2 class="switch__title title">Welcome Back !</h2>
        <p class="switch__description description">To keep connected with us please login with your personal info</p>
        <button class="switch__button button switch-btn">SIGN IN</button>
        <br><br><br>
        <h3 style="color: rgb(186, 71, 71) ;"><%= alert %> </h3>
      </div>
      <div class="switch__container is-hidden" id="switch-c2">
        <h2 class="switch__title title">Hello Friend !</h2>
        <p class="switch__description description">Enter your personal details and start journey with us</p>
        <button class="switch__button button switch-btn">SIGN UP</button>
      </div>
    </div>
  </div>
  <!-- partial -->
  <script src="/static/login-signup/script.js"></script>

</body>

</html>